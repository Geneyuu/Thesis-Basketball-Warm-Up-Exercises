{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts",
		"app/(tabs)/home/Home.jsx",
		"app/(tabs)/_layout.jsx",
		"app/_layout.jsx",
		"app/(tabs)/Profile.jsx",
		"app/(tabs)/Settings/index.jsx",
		"app/index.jsx"
	]
}
